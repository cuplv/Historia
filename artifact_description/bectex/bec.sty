% define colors
\ifcsname definecolor\endcsname%
  \definecolor{dullred}{Hsb}{0,1,0.4}
  \definecolor{dullyellow}{Hsb}{30,1,0.4}
  \definecolor{dullgreen}{Hsb}{60,1,0.4}
  \definecolor{dullteal}{Hsb}{150,1,0.4}
  \definecolor{dullblue}{Hsb}{210,1,0.4}
  \definecolor{dullpurple}{Hsb}{270,1,0.4}
  \definecolor{dullmagenta}{Hsb}{300,1,0.4}
  \definecolor{dullpurplered}{Hsb}{330,1,0.4}
  \newcommand{\colordullselect}[1]{%
    \ifnum0=#1
    dullred%
    \else\ifnum1=#1
    dullteal%
    \else\ifnum2=#1
    dullmagenta%
    \else\ifnum3=#1
    dullyellow%
    \else\ifnum4=#1
    dullblue%
    \else\ifnum5=#1
    dullgreen%
    \else\ifnum6=#1
    dullpurple%
     \else\ifnum7=#1
    dullpurplered%
    \fi\fi\fi\fi\fi\fi\fi\fi
  }

  \newcommand{\colordull}[1][0]{\color{\colordullselect{#1}}}
  \newcommand{\colordullx}[1]{\color{\colordullselect{#1}}}
  \newcommand{\colordullul}[2][0]{\underline{\colordullx{#1}{\color{black}#2}}}
  \newcommand{\textcolordull}[2][0]{\textcolor{\colordullselect{#1}}{#2}}

  \newcommand{\emcolorcolor}{dullteal}
  \newcommand{\emcolor}{\color{\emcolorcolor}}
  \newcommand{\textemcolor}[1]{\textcolor{\emcolorcolor}{#1}}

  \definecolor{dullredrgb}{rgb}{0.4,0,0}

  \definecolor{greytwentyfive}{Hsb}{0,0,0.25}
  \definecolor{greyfifty}{Hsb}{0,0,0.5}
  \definecolor{greyseventyfive}{Hsb}{0,0,0.75}

  \newcommand{\hibox}[1]{%
    \fcolorbox{white}{black!20}{$#1$}%
  }
  
  % used by diff
  \definecolor{lightredbg}{rgb}{1.0,0.93,0.93}
  \definecolor{lightgreenbg}{rgb}{0.92,1.0,0.92}
  \definecolor{lightyellowbg}{rgb}{1.0,1.0,0.88}
