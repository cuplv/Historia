%
% Abstract Interpretation
%

\newcommand{\AIbot}[1][]{\lbot_{#1}}
\newcommand{\AItop}[1][]{\ltop_{#1}}
\newcommand{\AIle}[1][]{\sqsubseteq_{#1}}
\newcommand{\AIwiden}[1][]{\triangledown_{#1}}
\newcommand{\AIWiden}[1][]{\bigtriangledown_{#1}}
\newcommand{\AIlub}[1][]{\sqcup_{#1}}
\newcommand{\AILub}[1][]{\bigsqcup_{#1}}
\newcommand{\AIub}[1][]{\check{\sqcup}_{#1}}
\newcommand{\AIUb}[1][]{\check{\bigsqcup}_{#1}}
\newcommand{\AIjoin}[1][]{\AIlub[#1]}
\newcommand{\AIJoin}[1][]{\AILub[#1]}
\newcommand{\AIglb}[1][]{\sqcap_{#1}}
\newcommand{\AIGlb}[1][]{\bigsqcap_{#1}}
\newcommand{\AImeet}[1][]{\AIglb[#1]}
\newcommand{\AIMeet}[1][]{\AIGlb[#1]}
\newcommand{\AIstep}[1][]{\rightarrow_{#1}}
\newcommand{\AIsound}[1][]{\simeq_{#1}}

\newcommand{\AIop}[2][]{\matheur{#2}_{#1}}
\newcommand{\AIguard}[1][]{\AIop[#1]{guard}}
\newcommand{\AIprove}[1][]{\AIop[#1]{prove}}


\ifx\upalpha\undefined
  % no upgreek letters
  \newcommand{\AIabsfn}{\alpha}
  \newcommand{\AIabselementfn}{\beta}
  \newcommand{\AIconcfn}{\gamma}
\else
  % upgreek letters (load upgreek package)
  \newcommand{\AIabsfn}{\upalpha}
  \newcommand{\AIabselementfn}{\upbeta}
  \newcommand{\AIconcfn}{\upgamma}
\fi

\newcommand{\AIabs}[1]{#1^{\boldsymbol{\sharp}}}

\newcommand{\absof}[1]{\ensuremath{\widehat{\text{\relscale{0.9}$#1$}}}}
\newcommand{\absofset}[1]{\ensuremath{\hat{#1}}}
\newcommand{\seq}[1]{\ensuremath{(#1)}}
\newcommand{\seqof}[1]{\ensuremath{#1^{\ast}}}
\newcommand{\nonemptyseqof}[1]{\ensuremath{#1^{+}}}
\newcommand{\infseqof}[1]{\ensuremath{#1^{\omega}}}

\DeclareMathOperator{\lfp}{lfp}
\DeclareMathOperator{\gfp}{gfp}

