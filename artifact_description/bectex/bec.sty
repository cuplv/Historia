%%% Do interesting chapter/section referencing (code from the web).
\def\breakref#1.#2\relax#3\relax{\def\xchap{#1}\def\xsec{#2}}
\def\chsecref#1{%
  \protected@edef\@tempa{\protect\breakref\ref{#1}\relax.\relax\relax}%
  \@tempa
  \protected@edef\xxchap{\thechapter}%
  \ifx\xchap\xxchap
     \secreftext~\xsec\ in this \chreftext
  \else
     \chreftext~\xchap.\xsec
  \fi}

